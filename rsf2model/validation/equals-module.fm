Item FB tristate
Item FB_FOOBAR boolean
Item FB_BARFOO boolean
Depends FB_FOOBAR "FB=y || FB=n"
Depends FB_BARFOO "FB!=n"

Item	ISDN_CAPI	tristate	menuconfig

Item	GIGASET_CAPI	boolean
Depends	GIGASET_CAPI	"ISDN && ISDN_DRV_GIGASET && ISDN_DRV_GIGASET=m"

Item    GIGASET_WASABI  boolean
Depends GIGASET_WASABI "ISDN_CAPI && FB!=y"
/*
 * check-name: =n, =y, =m...
 * check-output-start
I: Items-Count: 8
I: Format: <variable> [presence condition]
CONFIG_FB !CONFIG_FB_MODULE
CONFIG_FB_BARFOO ((CONFIG_FB_MODULE || CONFIG_FB))
CONFIG_FB_FOOBAR (CONFIG_FB || (!CONFIG_FB_MODULE && !CONFIG_FB))
CONFIG_FB_MODULE !CONFIG_FB && CONFIG_MODULES
CONFIG_GIGASET_CAPI (CONFIG_ISDN && CONFIG_ISDN_DRV_GIGASET && CONFIG_ISDN_DRV_GIGASET_MODULE)
CONFIG_GIGASET_WASABI ((CONFIG_ISDN_CAPI_MODULE || CONFIG_ISDN_CAPI) && !CONFIG_FB)
CONFIG_ISDN_CAPI !CONFIG_ISDN_CAPI_MODULE
CONFIG_ISDN_CAPI_MODULE !CONFIG_ISDN_CAPI && CONFIG_MODULES
 * check-output-end
 */

