config X86_INTEL_MID
	bool

config X86_MRST
       bool "Moorestown MID platform"
	depends on PCI
	depends on PCI_GOANY
	depends on X86_IO_APIC
	select APB_TIMER
	select I2C
	select SPI
	select INTEL_SCU_IPC
