config MODULES
	bool 
	default y

config F1
	tristate "F1"

config F2
	tristate "F2"

config F3
	tristate "F3"


config F4
	bool "F4"

config F5
	bool "F5"

config F6
	bool "F6"

config X1
	tristate "X1"
	depends on F1 || F2

config X2
	tristate "X2"
	depends on F1 && F2

config X3
	tristate "X3"
	depends on F1 = m

config X4
	tristate "X4"
	depends on (F1 = m) || (F1 = y)

config X5
	tristate "X5"
	depends on (F1  || F2) && F3

config Y1
	tristate "Y1"
	depends on (X1  && F2) || F3

config Z1
	tristate "Z1"
	depends on F4 ||F5
